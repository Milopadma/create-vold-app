{
  "root": true,
  "extends": ["@nuxt/eslint-config", "plugin:prettier/recommended"],
  "rules": {
    "vue/require-default-prop": "off",
    "vue/no-multiple-template-root": "off",
    "semi": [2, "always"],
    "no-new": 0,
    "space-before-function-paren": "off",
    "no-console": "off",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
